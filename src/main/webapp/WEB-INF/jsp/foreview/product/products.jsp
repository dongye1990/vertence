<script type="text/javascript">
$(function(){
    $("#product"+${type}).removeClass('product_hidden');
    $(".ygsjd_btnbox span").eq(${type}).addClass('btn_active');
    $(".ygsjd_btnbox span").click(function() {
        $(this).siblings('span').removeClass('btn_active');
        $(this).addClass('btn_active');
        $.each(new Array(5), function(index) {
             $("#product"+index).addClass('product_hidden');
         });
        $("#product"+$(this).index()).removeClass('product_hidden');
    });
});
</script>